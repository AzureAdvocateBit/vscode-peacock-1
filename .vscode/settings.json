// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "out": false // set this to true to hide the "out" folder with the compiled JS files
  },
  "search.exclude": {
    "out": true // set this to false to include "out" folder in search results
  },
  // Turn off tsc task auto detection since we have the necessary tasks as npm scripts
  "typescript.tsc.autoDetect": "off",
  "workbench.colorCustomizations": {
    "window.activeBorder": "#174a8e",
    "activityBar.background": "#0be90f",
    "activityBar.activeBackground": "#0be90f",
    "activityBar.activeBorder": "#6967f8",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#6967f8",
    "activityBarBadge.foreground": "#e7e7e7",
    "titleBar.activeBackground": "#09b80c",
    "titleBar.inactiveBackground": "#09b80c99",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#09b80c",
    "statusBarItem.hoverBackground": "#0be90f",
    "statusBar.foreground": "#e7e7e7",
    "panel.border": "#0be90f",
    "sideBar.border": "#0be90f",
    "editorGroup.border": "#0be90f",
    "statusBar.border": "#09b80c",
    "titleBar.border": "#09b80c"
  },
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "peacock.color": "#09b80c"
}
